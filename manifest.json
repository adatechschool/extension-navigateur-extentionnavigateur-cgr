{
    "manifest_version": 3,
    "name": "Extension test recherche Google / Wiki",
    "version": "1.0",
    "description": "Cette extension permet de rechercher des choses",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/ada16.png",
            "24": "images/ada24.png",
            "32": "images/ada32.png",
            "48": "images/ada48.png",
            "128": "images/ada128.png"
        }
    }
}